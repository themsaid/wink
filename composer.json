{
    "name": "writingink/wink",
    "description": "Wink Publishing Framework.",
    "keywords": ["framework", "laravel", "blog"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "mohamed@laravel.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.7",
        "laravel/framework": "~5.7",
        "mockery/mockery": "^1.2",
        "orchestra/database": "^3.6",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Wink\\Tests\\": "tests/"
      }
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.3.8"
    },
    "autoload": {
        "psr-4": {
            "Wink\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wink\\WinkServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
